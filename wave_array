#include<iostream>
using namespace std;
int main()

{
	int n,arr[n],temp;
	cin>>n;
	for(int i=1;i<=n;i++)
	{
		cin>>arr[i];
	}
	for(int i=1;i<=n;i++)
	{
		cout<<arr[i]<<" ";
	}
	cout<<endl;
	for(int i =1;i<=n;)
	{
		temp=arr[i];
		arr[i]=arr[i+1];
		arr[i+1]=temp;
		cout<<arr[i]<<" ";
		cout<<arr[i+1]<<" ";
		i=i+2;
		
	}
	
	
}
