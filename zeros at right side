#include<iostream>
using namespace std;
int main()
{
	int n,arr[n],temp;
	cin>>n;
	for(int i=1;i<=n;i++)
	{
		cin>>arr[i];
	}
	for(int i=1;i<=n;i++)
	{
		if(arr[i]==0)
		{
			temp=arr[n];
			arr[n]=arr[i];
			arr[i]=temp;
			//i++;
			n=n-1;
		}
		cout<<arr[i]<<" ";
	}
	for(int i=1;i<=n;i++)
	{
		cout<<arr[i]<<" ";
	}
}
